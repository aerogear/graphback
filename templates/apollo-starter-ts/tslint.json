{
  "extends": "tslint:recommended",
  "rules": {
      "max-line-length": {
          "options": [120]
      },
      "new-parens": true,
      "no-arg": true,
      "no-bitwise": true,
      "no-conditional-assignment": false,
      "no-consecutive-blank-lines": false
  },
  "jsRules": {
      "max-line-length": {
          "options": [120]
      }
  }
}