{
  "name": "graphql-migrations",
  "version": "0.11.0-rc3",
  "description": "Create & migrate Databases from GraphQL Schema",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "typescript": {
    "definition": "types/index.d.ts"
  },
  "scripts": {
    "clean": "rimraf coverage dist types",
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint -t stylish --project \"tsconfig.json\"",
    "test": "ava",
    "pretest": "yarn build"
  },
  "ava": {
    "files": [
      "tests/**/*"
    ],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "@graphback/core": "0.11.0-rc3",
    "@graphql-inspector/core": "1.27.0",
    "case": "1.6.2",
    "chalk": "3.0.0",
    "glob": "7.1.6",
    "graphql-annotations": "0.0.3",
    "knex": "0.20.10",
    "lodash": "4.17.15",
    "lodash-es": "4.17.15",
    "node-emoji": "1.10.0",
    "pino": "5.16.0"
  },
  "devDependencies": {
    "@types/jest": "25.1.3",
    "@types/lodash": "4.14.149",
    "@types/node": "12.12.28",
    "ava": "2.4.0",
    "graphql": "14.6.0",
    "graphql-tools": "4.0.7",
    "pg": "7.18.2",
    "rimraf": "3.0.2",
    "sqlite3": "4.1.1",
    "ts-node": "8.6.2",
    "tsutils": "3.17.1",
    "typescript": "3.8.2"
  },
  "peerDependencies": {
    "graphql": "^0.12.0 || ^0.13.0 || ^14.0.0"
  },
  "engines": {
    "node": ">=10.15.3"
  }
}
