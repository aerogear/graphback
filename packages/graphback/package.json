{
  "name": "graphback",
  "version": "0.2.0",
  "description": "Auto generate resolvers on top of the graphql and database of your choice",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "author": "wtrocki@redhat.com",
  "scripts": {
    "clean": "rimraf coverage dist types",
    "build": "tsc && cp -Rf ./src/schema/partials ./dist/schema/partials",
    "watch": "tsc -w ",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "test": "ava",
    "pretest": "npm run build"
  },
  "ava": {
    "files": [
      "tests/**/*"
    ],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "engines": {
    "node": "12.4.0"
  },
  "devDependencies": {
    "@types/glob": "5.0.35",
    "@types/graphql": "0.13.4",
    "@types/handlebars": "4.0.39",
    "@types/knex": "0.14.24",
    "@types/node": "8.10.25",
    "@types/pino": "5.20.0",
    "ava": "1.4.1",
    "rimraf": "2.6.2",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tsutils": "3.0.0",
    "typescript": "3.0.3"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "glob": "7.1.3",
    "graphql": "0.13.2",
    "graphql-code-generator": "0.18.2",
    "graphql-codegen-core": "0.18.2",
    "graphql-import": "0.7.0",
    "graphql-tools": "3.1.1",
    "handlebars": "4.0.14",
    "knex": "0.15.2",
    "pg": "^7.11.0",
    "pino": "5.5.0",
    "tslib": "1.9.3"
  }
}
