{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "CLI generate",
      "program": "${workspaceRoot}/packages/graphback-cli/dist/index.js",
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
      "sourceMaps": true,
      "protocol": "inspector",
      "args": [
        "generate"
      ],
      "cwd": "${workspaceRoot}/templates/apollo-starter-ts",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "CLI db",
      "program": "${workspaceRoot}/packages/graphback-cli/dist/index.js",
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
      "sourceMaps": true,
      "protocol": "inspector",
      "args": [
        "db"
      ],
      "cwd": "${workspaceRoot}/templates/apollo-starter-ts",
    }
  ]
}