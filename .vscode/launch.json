{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Current TS File",
      "type": "node",
      "request": "launch",
      "args": [
        "${relativeFile}"
      ],
      "runtimeArgs": [
        "-r",
        "ts-node/register"
      ],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector",
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "name": "Template app Postgres",
      "type": "node",
      "request": "launch",
      "env": {
        "DB_CLIENT": "pg",
        "DB_USER": "postgresql",
        "DB_PASSWORD": "postgres",
        "DB_DATABASE": "users",
        "DB_HOST": "localhost",
        "DB_PORT": "55432"
      },
      "runtimeArgs": [
        "-r",
        "ts-node/register"
      ],
      "cwd": "${workspaceRoot}/templates/ts-apollo-postgres-backend",
      "protocol": "inspector",
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "CLI config",
      "program": "${workspaceRoot}/packages/graphback-cli/dist/index.js",
      "runtimeArgs": [
        "--nolazy",
        "-r",
        "ts-node/register"
      ],
      "sourceMaps": true,
      "console": "integratedTerminal",
      "protocol": "inspector",
      "args": [
        "config"
      ],
      "cwd": "${workspaceRoot}/templates/ts-apollo-postgres-backend",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "CLI generate",
      "program": "${workspaceRoot}/packages/graphback-cli/dist/index.js",
      "runtimeArgs": [
        "--nolazy",
        "-r",
        "ts-node/register"
      ],
      "sourceMaps": true,
      "protocol": "inspector",
      "args": [
        "generate"
      ],
      "cwd": "${workspaceRoot}/templates/ts-apollo-postgres-backend",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "CLI serve",
      "program": "${workspaceRoot}/packages/graphql-serve/dist/index.js",
      "runtimeArgs": [
        "--nolazy",
        "-r",
        "ts-node/register"
      ],
      "sourceMaps": true,
      "protocol": "inspector",
      "args": [
        "serve",
        "port=8080"
      ],
      "cwd": "${workspaceRoot}/templates/ts-apollo-postgres-backend",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Jest Current File",
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "args": [
        "${fileBasenameNoExtension}",
        "--config",
        "jest.config.js"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "disableOptimisticBPs": true,
      "windows": {
        "program": "${workspaceFolder}/node_modules/jest/bin/jest",
      }
    }
  ]
}
